<div class="temporarily-hidden-content" data-year="{{YEAR}}" data-month="{{MONTH}}" data-day="{{DAY}}" data-hour="{{HOUR}}" data-minute="{{MINUTE}}" data-second="{{SECOND}}" data-timestamp="{{TIMESTAMP}}" data-color="{{COLOR}}">
	<div class="temphc-countdown"></div>
	<p class="temphc-message"></p>
	{IF {{MESSAGE}}}
		<p class="temphc-message">{{MESSAGE}}</p>
	{END IF}
</div>